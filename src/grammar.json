{
  "name": "Clojure",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "sExpression"
      }
    },
    "sExpression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "op",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "expression"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sExpression"
        },
        {
          "type": "SYMBOL",
          "name": "_collection"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "_literal"
        }
      ]
    },
    "_collection": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "vector"
        }
      ]
    },
    "vector": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "expression"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[:\\-]?[a-zA-Z_][a-zA-Z0-9_?\\-]*"
    },
    "_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "stringLiteral"
        }
      ]
    },
    "stringLiteral": {
      "type": "PATTERN",
      "value": "\"[^\\r\\n]*?\""
    }
  },
  "extras": [
